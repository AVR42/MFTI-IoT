#include <stdio.h>

int main(int argc, char **argv)
{
	int N;
	char c;
	scanf("%d", &N);
	while (scanf("%c", &c) == 1 && c != '.') {
		if (c == ' ') {
			printf(" ");
			}
		else if ('A' <= c && c <= 'Z') {
			printf("%c", 'A' + (c - 'A' + N) % 26);
			}
		else if ('a' <= c && c <= 'z') {
		printf("%c", 'a' + (c - 'a' + N) % 26);
		}
	}
	printf(".");
	return 0;
}

